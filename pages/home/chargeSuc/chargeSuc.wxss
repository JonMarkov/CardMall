/* css部分引入模版 */
@import "../list-tmpl/list-tmpl.wxss";
page {
  background: #ccc;
}

.concent {
  background: #fff;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  padding: 80rpx 40rpx;
}

.conImg {
  width: 18%;
}

.conDet {
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  line-height: 100rpx;
}

.conImg image {
  width: 100rpx;
  height: 210rpx;
}

.conSuc {
  font-size: 42rpx;
  margin-top: 10rpx;
  color: #ef6a1d
}

.conCon {
  font-size: 30rpx;
  color: #8b8b99
}

.wheel {
  margin-top: 30rpx;
  background: #f0f0f0;
  padding-top: 20rpx;
  height: 800rpx;
}

swiper{
  height: 550rpx;
  background: #fff;
  margin-top: 30rpx
}
swiper image {
  width: 100%;
  height: 350rpx;
}

.clan {
  display: flex;
  justify-content: space-between;
  background: #ffffff
}
.list-con{
  margin-bottom: 20rpx;
  border: 1px solid #e5e5e5;
  border-radius: 6rpx;
  width: 96%;
  text-align: center;
  margin: auto;
  /* /* margin-left: 30rpx; */
  margin-top: 20rpx; 
}
.clan-cate {
  width: 200rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  padding: 20rpx;
}

.clan-cate image {
  width: 100rpx;
  height: 100rpx;
}

.clan-cate text {
  font-size: 24rpx;
  margin-top: 20rpx;
  color: #cbcbcb
}
.returnBtn{
  background:#fff;
padding-bottom:60rpx;
display: flex;
flex-direction: row
}
.returnBtn button{
width:40%;
color:#fff;
}
.btn_one{
  background:#30b0ef;
}
.btn_two{
  background: #ccc
}